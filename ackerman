Programa uwu

	(define (ackerman m n)( 	
		(   
			if ((= m 0))
			 	((+ n 1))
			     else (
			    	(
			    		if ((+ n 1))
				   		   (	(ackerman (- m 1) 1)	)
				   			 else(
				   				(ackerman (- m 1) (ackerman m (- n 1) )	)
				   					
				   			)
				   	)		
				)	  
		 
		
							)
	)	)

	
	
	
	
